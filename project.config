[project]
	description = my first repository
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/7t
[access "refs/for/refs/*"]
	read = group user/7t
	push = group user/7t
	label-Code-Review = -2..+2 group user/7t
	label-Verified = -1..+1 group user/7t
	rebase = group user/7t
	removeReviewer = group user/7t
	submit = group user/7t
[access "refs/heads/*"]
	read = group user/7t
	create = group user/7t
	push = +force group user/7t
	pushMerge = group user/7t
[access "refs/tags/*"]
	push = +force group user/7t
	createTag = group user/7t
	createSignedTag = group user/7t
